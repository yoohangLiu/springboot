<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xs.boot.dao.ExamMapper">
    <insert id="insert" parameterType="Exam">
        insert into exam(id,exam_num,exam_type) values(#{id},#{exam_num},#{exam_type})
    </insert>

    <select id="findExam" parameterType="Exam" resultType="Exam">
        select id, exam_num, exam_type from exam where exam_num=#{exam_num} and exam_type=#{exam_type}
    </select>
</mapper>